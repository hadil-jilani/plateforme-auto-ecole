{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/backend/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/backend/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/backend",
  "projects": {
    "backend": {
      "type": "application",
      "root": "apps/backend",
      "entryFile": "main",
      "sourceRoot": "apps/backend/src",
      "compilerOptions": {
        "tsConfigPath": "apps/backend/tsconfig.app.json"
      }
    },
    "auth": {
      "type": "application",
      "root": "apps/auth",
      "entryFile": "main",
      "sourceRoot": "apps/auth/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth/tsconfig.app.json"
      }
    },
    "shared": {
      "type": "library",
      "root": "libs/shared",
      "entryFile": "index",
      "sourceRoot": "libs/shared/src",
      "compilerOptions": {
        "tsConfigPath": "libs/shared/tsconfig.lib.json"
      }
    },
    "api": {
      "type": "application",
      "root": "apps/api",
      "entryFile": "main",
      "sourceRoot": "apps/api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api/tsconfig.app.json"
      }
    },
    "requests": {
      "type": "application",
      "root": "apps/requests",
      "entryFile": "main",
      "sourceRoot": "apps/requests/src",
      "compilerOptions": {
        "tsConfigPath": "apps/requests/tsconfig.app.json"
      }
    },
    "email": {
      "type": "application",
      "root": "apps/email",
      "entryFile": "main",
      "sourceRoot": "apps/email/src",
      "compilerOptions": {
        "tsConfigPath": "apps/email/tsconfig.app.json"
      }
    },
    "trainers": {
      "type": "application",
      "root": "apps/trainers",
      "entryFile": "main",
      "sourceRoot": "apps/trainers/src",
      "compilerOptions": {
        "tsConfigPath": "apps/trainers/tsconfig.app.json"
      }
    },
    "profile": {
      "type": "application",
      "root": "apps/profile",
      "entryFile": "main",
      "sourceRoot": "apps/profile/src",
      "compilerOptions": {
        "tsConfigPath": "apps/profile/tsconfig.app.json"
      }
    },
    "occurrences": {
      "type": "application",
      "root": "apps/occurrences",
      "entryFile": "main",
      "sourceRoot": "apps/occurrences/src",
      "compilerOptions": {
        "tsConfigPath": "apps/occurrences/tsconfig.app.json"
      }
    },
    "agenda": {
      "type": "application",
      "root": "apps/agenda",
      "entryFile": "main",
      "sourceRoot": "apps/agenda/src",
      "compilerOptions": {
        "tsConfigPath": "apps/agenda/tsconfig.app.json"
      }
    },
    "learners": {
      "type": "application",
      "root": "apps/learners",
      "entryFile": "main",
      "sourceRoot": "apps/learners/src",
      "compilerOptions": {
        "tsConfigPath": "apps/learners/tsconfig.app.json"
      }
    }
  }
}